from django.contrib import admin
from .models import MyModel

@admin.register(MyModel)
class MyModelAdmin(admin.ModelAdmin):
    actions = ['delete_all_entries']  # Register custom action

    @admin.action(description='Delete ALL entries')
    def delete_all_entries(self, request, queryset):
        MyModel.objects.all().delete()  # Deletes all entries in the model
        self.message_user(request, "All entries deleted successfully.")
